let PageDir = Request.ServerVariables.URL;
Debug.LogInfo("PageDir: " + PageDir);

const RedirectPage = (function(Key) {
	return {
		"/" : "views/nav.esp",
		"/admin" : "views/admin.esp",
		"/css/all" : "views/css.esp",
		"/js/all" : "views/js.esp",
		"templates/images/codisto-logo-chain.png" : "templates/images/codisto-logo-chain.png"
	}[Key];
}());

if (RedirectPage) {
	Debug.LogInfo("Redirecting to : " + RedirectPage);
	Server.Transfer(RedirectPage);
}

